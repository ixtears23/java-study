<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
                <pattern>%-5level %logger{35} - %message%n</pattern>
        </encoder>
    </appender>

    <logger name="junseok.snr.study" level="DEBUG" />

    <root level="DEBUG">
        <appender-ref ref="console" />
    </root>

</configuration>